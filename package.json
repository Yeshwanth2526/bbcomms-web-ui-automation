{
  "name": "webdriverio",
  "description": "Project  to run WebdriverIO tests with Appium to test native applications on iOS and Android",
  "scripts": {
    "test2": "npx wdio config/wdio.conf.ts",
    "test": "ENV=QA1 && npm run report-ci && npx wdio config/wdio.conf.ts && npm run allure-report",
    "test3": "ENV=QA npm run report-ci && npm run delete && npx wdio config/wdio.conf.ts --mochaOpts.grep 'SMOKE' && npm run allure-report",
    "test4": "npm run report-ci && npm run delete && npx wdio config/wdio.conf.ts --mochaOpts.g 'TEST' && npm run allure-report",
    "docker-up": "docker-compose up",
    "docker-down": "docker-compose down",
    "multi-browser": "npm run report-ci && npx wdio config/wdio.conf.cross-browser.ts && npm run allure-report",
    "lint": "eslint config tests",
    "allure-report": "allure generate ./results/allure-raw --clean && allure open",
    "report-ci": "allure generate ./results/allure-raw --clean -o allure-report",
    "cleandir": "cleandir ./results/",
    "test1": "docker compose run --rm wdio config/wdio.conf.ts",
    "delete": "del-cli --force ./Reports/**.html",
    "test5": "npm run report-ci && npm run test:run -- --suite=regression --env=stage && npm run allure-report || npm run allure-report"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "WebdriverIO",
    "WebDriver"
  ],
  "author": "Wim Selles <wim@wswebcreation.nl>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/faker": "^6.6.8",
    "@types/jquery": "^3.5.14",
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@wdio/allure-reporter": "^7.25.1",
    "@wdio/cli": "^7.25.1",
    "@wdio/local-runner": "^7.25.1",
    "@wdio/mocha-framework": "^7.25.1",
    "@wdio/sauce-service": "^7.16.13",
    "@wdio/selenium-standalone-service": "^7.31.1",
    "@wdio/spec-reporter": "^7.25.1",
    "chai": "^4.3.6",
    "eslint-plugin-wdio": "^7.21.0",
    "faker": "^5.5.3",
    "jquery": "^3.6.1",
    "mochawesome": "^7.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "wdio-safaridriver-service": "^2.0.0",
    "wdio-selenium-standalone-service": "^0.0.12"
  },
